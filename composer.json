{
    "name": "larva/think-hashing",
    "description": "The ThinkPHP Hashing package.",
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "topthink/framework": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Larva\\Hashing\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "think": {
            "services": [
                "larva\\Hashing\\HashServiceProvider"
            ],
            "config":{
                "hashing": "config/hashing.php"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
